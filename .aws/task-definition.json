{
  "family": "epam_task_definition_name",
  "executionRoleArn": "arn:aws:iam::021891605050:role/EcsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::021891605050:role/EcsTaskExecutionRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "kata1_container",
      "image": "021891605050.dkr.ecr.us-west-2.amazonaws.com/ewardq/epam_python:latest",
      "essential": true,
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/epam_task_definition_name",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": [],
      "environmentFiles": [],
      "mountPoints": [],
      "systemControls": [],
      "ulimits": [],
      "volumesFrom": []
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "2048",
  "volumes": [],
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}

